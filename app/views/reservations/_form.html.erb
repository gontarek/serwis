<%= form_for(@reservation) do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :phone %>
  <%= f.text_field :phone %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :desc %>
  <%= f.text_field :desc %>

  <%= f.label :date %>
  <%= f.datetime_field :date %>

  <%= f.submit %>
<% end %>
