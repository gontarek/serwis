<%= form_for(@reservation) do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %><br />

  <%= f.label :phone %>
  <%= f.text_field :phone %><br />

  <%= f.label :name %>
  <%= f.text_field :name %><br />

  <%= f.label :desc %>
  <%= f.text_field :desc %><br />

  <%= f.label :date %>
  <%= f.datetime_select(:date) %><br />

  <%= f.submit %>
<% end %>
